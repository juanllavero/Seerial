            <Font size="18.0" />
         </font>
         <effect>
            <DropShadow offsetY="2.5" />
         </effect>
      </Label>
   </children>
   <styleClass>
      <String fx:value="transparent" />
      <String fx:value="roundButton" />
   </styleClass>
</VBox>
