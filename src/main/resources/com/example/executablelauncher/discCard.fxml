         <VBox.margin>
            <Insets top="5.0" />
         </VBox.margin>
         <effect>
            <DropShadow offsetY="2.5" />
         </effect>
      </Label>
      <Label fx:id="number" alignment="CENTER" contentDisplay="CENTER" text="Episode 15" textFill="#e8e8e8">
